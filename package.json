{
  "name": "pwa-redux-skeleton",
  "version": "0.0.1",
  "description": "A simple starter skeleton for a PWA playground",
  "main": "index.js",
  "scripts": {
    "webpack": "cross-env webpack",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-core/register --reporter spec ./**/__test__/*.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js --color -p",
    "build:clean": "cross-env rimraf ./dist/*",
    "build:copyStatic": "rsync -r manifest.json browserconfig.xml favicon.ico app/images ./dist",
    "build:swCache": "sw-precache --config=build/sw-precache-config.json",
    "start": "cross-env NODE_ENV=development node ./src/server",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "echo \"No css lint enabled. U suck dude\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobangel/pwa-starter-skeleton"
  },
  "keywords": [
    "kewl",
    "cool",
    "coolbeans"
  ],
  "author": "jangel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobangel/pwa-starter-skeleton/issues"
  },
  "homepage": "https://github.com/jacobangel/pwa-starter-skeleton",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "ip": "^1.1.3",
    "lodash": "^4.13.1",
    "minimist": "^1.2.0",
    "ngrok": "^2.2.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "postcss": "^5.0.21",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "sw-precache": "^3.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0"
  }
}
